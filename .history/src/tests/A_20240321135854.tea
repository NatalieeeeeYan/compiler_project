fn Dijkstra()
{
    let i:int;
    i = 1;
    while (i <= n) {
        dis[i] = e[1].a[i];
        book[i] = 0;
        i = i + 1;
    }
    book[1] = 1;

    i = 1;
    while (i <= (n - 1)) {
        let min_num:int = INF;
        let min_index:int = 0;
        let k:int = 1;
        while (k <= n) {
            if (min_num > dis[k] && book[k] == 0) {
                min_num = dis[k];
                min_index = k;
            }
            k = k + 1;
        }
        book[min_index] = 1;
        let j:int = 1;
        while (j <= n) {
            if (e[min_index].a[j] < INF) {
                if (dis[j] > (dis[min_index] + e[min_index].a[j])) {
                    dis[j] = dis[min_index] + e[min_index].a[j];
                }
            }
            j = j + 1;
        }
        i = i + 1;
    }
}
